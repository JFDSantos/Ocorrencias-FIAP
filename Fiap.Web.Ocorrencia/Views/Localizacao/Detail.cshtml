                <span>@Model.cidade</span>
            </div>
        </div>
        <br />
        <div class="form-group">
            <label asp-for="cep" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.cep</span>
            </div>
        </div>
        <br />
